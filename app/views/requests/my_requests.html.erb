<div class="container">
  <h1>My Collaboration Track Requests: </h1>
  <% @requests.each do |request| %>
    <p>Description: <%= request.description %></p>
    <%= render 'shared/track', track: request.track %>
    <%= link_to 'Delete request', request_path(request), method: :delete, data: { confirm: "Are you sure you want to Delete your request?" }, class: "btn btn-dark" %>
  <% end %>
</div>
