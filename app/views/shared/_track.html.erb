
  <div class="track-card-header">
    <h4><%= track.name %></h1>
    <h3>By: <%= track.user.username %></h2>
  </div>
<div class="track-card-hull">

  <div class="track-card-wav">
    <div id="track_<%= track.id %>" class="<%= "waveform" if track.file %>" data-track-url='<%= track.file.url(secure: true) %>'></div>
  </div>

  <div class="track-wav-details">
    <p><strong>Length:  </strong><%= track.duration %>s</p>
    <p><strong>bpm:  </strong><%= track.bpm %></p>
    <p><strong>Time Signature:  </strong><%= track.time_signature %></p>
    <p><strong>Key:  </strong><%= track.key %></p>
  </div>
</div>


